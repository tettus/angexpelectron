{
  "name": "angexpelectron",
  "version": "1.0.0",
  "description": "Tettu",
  "main": "index.js",
  "author": "Sudharsan",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install && install-app-deps",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3001  --content-base src",
    "start": "electron .",
    "dev": "lite-server",
    "pack": "build --dir",
    "dist": "build"
  },
  "contributors": [
    "Sudharsan Tettu <sudharsan_tk@yahoo.co.in>"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4",
    "electron-prebuilt": "^1.3.1"
    },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12",
    "jquery": "^2.2.4",
    "hammerjs": "2.0.8",
    "materialize-css": "0.97.7",
    "angular2-materialize": "3.0.3",
    "express": "4.14",
    "mongoose": "4.5.8",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0"
  },
  "keywords": [
    "Angular2",
    "Electron",
    "Electron Angular Express seed",
    "Electron Express",
    "Electron Angular2",
